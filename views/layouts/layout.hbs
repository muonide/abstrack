<html>
	{{> head }}
	<body class="is-loading">
		<div id="wrapper">
			{{!-- if index, render intro and header --}}
			{{#if home}}
				{{> intro }}
				{{> header }}
			{{!-- else normal page, just render header --}}
			{{else}}
				{{> header }}
			{{/if}}
			{{> nav-bar }}
			<div id="main">
				{{{ body }}}
			</div>
			{{> footer }}
			{{> copyright }}
		</div>
		{{> scripts }}
	</body>
</html>
